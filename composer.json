{
  "name": "miaoxing/captcha",
  "description": "Miaoxing Captcha Plugin",
  "keywords": [
    "miaoxing", "captcha"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "dev-master@dev"
  },
  "require-dev": {
    "miaoxing/coding-standards": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Captcha\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Captcha\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0.x-dev"
    }
  }
}
