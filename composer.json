{
  "name": "miaoxing/captcha",
  "type": "miaoxing-plugin",
  "description": "Miaoxing Captcha Plugin",
  "keywords": [
    "miaoxing",
    "captcha"
  ],
  "license": "MIT",
  "require": {
    "miaoxing/plugin": "^0.12.2",
    "miaoxing/services": "^0.7.4"
  },
  "require-dev": {
    "miaoxing/dev": "^8.2.2"
  },
  "autoload": {
    "psr-4": {
      "Miaoxing\\Captcha\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MiaoxingTest\\Captcha\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "phpstan/extension-installer": true
    }
  },
  "extra": {
    "require-ci": {
      "miaoxing/dev": "miaoxing/dev as 8.x-dev",
      "miaoxing/phpstan-miaoxing": "miaoxing/phpstan-miaoxing as 1.x-dev"
    }
  }
}
